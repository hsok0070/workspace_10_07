package x.y;

public class GreetingServiceImpl implements OutputService {
	
	private OutputService outputter;

	public void setOutputter(OutputService outputter) {
		this.outputter = outputter;
	}

	@Override
	public void print() {
		System.out.println("hi");
	}

	public void a() {
		System.out.println(outputter);
	}

	
}
